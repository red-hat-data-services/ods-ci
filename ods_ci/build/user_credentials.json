{
    "idp":{
        "ldap": {
            "prefixes": [
                "rhods-adm-",
                "rhods-usr-",
                "rhods-noaccess-",
                "rhods-special-"
            ],
            "suffixes": {
                "rhods-adm-": {
                    "type": "incremental_with_rand_base",
                    "n_users": 20
                },
                "rhods-usr-": {
                    "type": "incremental_with_rand_base",
                    "n_users": 20
                },
                "rhods-noaccess-": {
                    "type": "incremental_with_rand_base",
                    "n_users": 20
                },
                "rhods-special-": {
                    "type": "custom_with_rand_base",
                    "list": [".","^","$","*","(",")","[","]","{","}","|","@",";"]
                }
            },
            "pw": "<GEN_RAMDOM_PW>",
            "TEST_USER": "rhods-adm-<RAND_BASE>11",
            "TEST_USER_2": "rhods-adm-<RAND_BASE>2",
            "TEST_USER_3": "rhods-usr-<RAND_BASE>3",
            "TEST_USER_4":  "rhods-usr-<RAND_BASE>4",
            "groups_map":{
                "rhods-adm-": [
                    "rhods-admins", 
                    "dedicated-admins"
                ],
                "rhods-usr-": [
                    "rhods-users"
                ],
                "rhods-noaccess-": [
                    "rhods-noaccess"
                ],
                "rhods-special-": [
                    "rhods-users"
                ]
            }
        },
        "htpasswd": {
            "prefixes": [
                "htpasswd-user-"
            ],
            "suffixes": {
                "rhods-htp-user-": {
                    "type": "incremental_with_rand_base",
                    "n_users": 1
                }
            },
            "pw": "<GEN_RAMDOM_PW>",
            "cluster_admin_username":   "rhods-htp-user-<RAND_BASE>"
        }
    }
}